<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rootlay"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/page_background"
    tools:context=".DartMainActivity">
    <FrameLayout
        app:layout_constraintTop_toTopOf="@+id/frmlay_score"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/frmlay_score"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintHeight_percent="0.1"
        app:layout_constraintWidth_percent="0.3">
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:src="@drawable/scoreboard1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            android:paddingBottom="10dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="목표"
                android:textColor="@color/black"
                android:fontFamily="@font/one_mobile_pop"/>
            <TextView
                android:id="@+id/target_score"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="300"
                android:textColor="@color/black"
                android:fontFamily="@font/one_mobile_pop"
                />
        </LinearLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/txt_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="0"
        android:textSize="20sp"
        android:textColor="@color/black"
        android:fontFamily="@font/one_mobile_pop"
        android:visibility="invisible"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/frmlay_score"
        app:layout_constraintBottom_toTopOf="@+id/txt_remain_score"
        app:layout_constraintVertical_chainStyle="packed"
        />

    <TextView
        android:id="@+id/txt_remain_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="남은 점수 : 100"
        android:textSize="12sp"
        android:textColor="@color/black"
        android:fontFamily="@font/one_mobile_pop"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_time"
        app:layout_constraintBottom_toBottomOf="@+id/frmlay_score"
        app:layout_constraintVertical_chainStyle="packed"
        />

